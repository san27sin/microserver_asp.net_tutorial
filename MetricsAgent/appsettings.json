{//файл настроек
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },//Дописываем настройки для нашего проекта
  "AllowedHosts": "*",
  "Settings": {//настройки
    "Logger": { //логер
      "Path": "C:/Logs/MetricsAgent" //путь к директории в рамках которой мы будем осуществлять логирование к частве нашего приложения
    },
    "DatabaseOptions": {
      "ConnectionString": "Data Source=metrics.db;Version=3;Pooling=true;Max Pool Size=100;"
    }
  }
}
 